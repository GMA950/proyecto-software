{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "name": {
      "type": "string"
    },
    "cat": {
      "type": "string"
    },
    "fab": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "ver": {
      "type": "string"
    },
    "year": {
      "type": "string"
    },
    "price": {
      "default": 0,
      "type": "integer"
    },
    "critic": {
      "type": "integer",
      "default": 1
    },
    "stock": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "stockevents": {
      "via": "product",
      "collection": "stockevent"
    },
    "ubicacion": {
      "type": "string",
      "default": "Bodega"
    },
    "origen": {
      "type": "string"
    },
    "nota": {
      "type": "text"
    }
  }
}
